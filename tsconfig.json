{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames":true,
    "allowJs": true,
    "noEmit": true,
    "types": ["vite/client"]
  },
  "include": ["index.ts", "packages", "packages/typetura/"]
}
